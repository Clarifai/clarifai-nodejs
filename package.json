{
  "name": "clarifai-nodejs",
  "version": "0.0.1",
  "description": "The official Clarifai Node.js SDK",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "scripts": {
    "test": "vitest",
    "test:unit": "vitest unit.test",
    "test:integration": "vitest run integration.test",
    "coverage": "vitest run --coverage",
    "lint:fix": "eslint . --fix --ignore-path .gitignore",
    "lint": "tsc --noEmit --pretty && eslint . --ignore-path .gitignore",
    "watch": "parcel watch",
    "build": "parcel build",
    "generate-docs": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-plugin-no-inherit --out docs src/"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Clarifai/clarifai-nodejs.git"
  },
  "keywords": [
    "clarifai",
    "clarifai-nodejs",
    "clarifai-typescript"
  ],
  "author": "Clarifai Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Clarifai/clarifai-nodejs/issues"
  },
  "homepage": "https://github.com/Clarifai/clarifai-nodejs#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.11.0",
    "@parcel/transformer-typescript-types": "^2.11.0",
    "@types/google-protobuf": "^3.15.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vitest/coverage-v8": "^1.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "parcel": "^2.11.0",
    "prettier": "3.2.4",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-plugin-no-inherit": "^1.4.0",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.1",
    "@grpc/proto-loader": "^0.7.10",
    "chalk": "^5.3.0",
    "clarifai-nodejs-grpc": "^10.0.9",
    "csv-parse": "^5.5.5",
    "google-protobuf": "^3.21.2",
    "js-yaml": "^4.1.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  }
}
